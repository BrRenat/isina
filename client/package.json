{
  "name": "isina-client",
  "version": "0.1.0",
  "main": "src/index.js",
  "repository": {
    "url": "",
    "type": ""
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "start": "cross-env NODE_ENV=development PORT=8080 node internals/server",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config internals/webpack/webpack.dev.babel.js",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "lint:eslint": "eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "author": "Renat Bererzovsky <renat0014@gmail.com>",
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/tests.{js,jsx}",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "7.1.1",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "jest-cli": "^22.1.4",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.4",
    "open": "^0.0.5",
    "pre-commit": "^1.2.2",
    "qrcode-terminal": "^0.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "styled-components": "^2.1.1",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "2.15.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-client-preset": "^1.0.6",
    "apollo-link": "^1.0.7",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "axios": "^0.16.2",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "lodash.compose": "^2.4.1",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "nanoid": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-act": "^1.5.1",
    "redux-form": "^7.2.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  }
